{
  "name": "@cryptico/aes",
  "description": "Typescript implementation of the AES block cipher",
  "version": "0.1.3",
  "author": {
    "name": "uamanager",
    "email": "faradey.ivan@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "registry": "https://registry.npmjs.org/",
  "private": false,
  "main": "lib/aes.js",
  "browser": "lib/aes.umd.js",
  "module": "lib/aes.esm.js",
  "typings": "lib/types/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptico-org/aes"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rollup -c rollup.config.ts",
    "commit": "git cz -a",
    "prepare": "husky install && yarn build",
    "postversion": "git push && git push --tags",
    "release": "standard-version && git push --follow-tags origin master && npm login --scope=@cryptico && npm publish --access public",
    "test": "yarn build && jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@rollup/plugin-commonjs": "25.0.2",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.18",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-typescript2": "0.34.1",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.0",
    "typescript": "5.1.3"
  },
  "resolutions": {
    "minimist": "1.2.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "cryptico",
    "aes",
    "key",
    "encrypt",
    "decrypt",
    "cryptography"
  ]
}
