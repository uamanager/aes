{
  "name": "@cryptico/aes",
  "description": "Typescript implementation of the AES block cipher",
  "version": "0.1.3",
  "author": {
    "name": "uamanager",
    "email": "faradey.ivan@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "registry": "https://registry.npmjs.org/",
  "private": false,
  "main": "lib/aes.js",
  "browser": "lib/aes.umd.js",
  "module": "lib/aes.esm.js",
  "typings": "lib/types/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptico-org/aes"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "rollup -c rollup.config.ts",
    "commit": "git cz -a",
    "prepare": "husky install && yarn build",
    "postversion": "git push && git push --tags",
    "release": "standard-version && git push --follow-tags origin master && npm login --scope=@cryptico && npm publish --access public",
    "test": "yarn build && jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@rollup/plugin-commonjs": "21.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@types/jest": "27.5.2",
    "@types/node": "17.0.25",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-junit": "13.2.0",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-typescript2": "0.34.1",
    "standard-version": "9.5.0",
    "ts-jest": "27.1.5",
    "typescript": "5.1.3"
  },
  "resolutions": {
    "minimist": "1.2.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "cryptico",
    "aes",
    "key",
    "encrypt",
    "decrypt",
    "cryptography"
  ]
}
